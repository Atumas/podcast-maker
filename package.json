{
    "name": "gobarber-backend",
    "version": "1.0.0",
    "main": "dist/server.js",
    "license": "MIT",
    "scripts": {
        "video:preview": "remotion preview video/src/index.tsx --props='{\"filename\": \"example\"}'",
        "video:remotion-debug": "remotion render video/src/index.tsx Main out.mp4 --props='{\"filename\": \"example\"}'",
        "video:make:tts": "ts-node src/main.ts --build --config tts",
        "video:make:youtube": "ts-node src/main.ts --build --config youtubeCreate ",
        "video:make:instagram": "ts-node src/main.ts --build --config instagramCreate ",
        "video:upload:youtube": "ts-node src/main.ts --build --config youtubeUpload",
        "video:upload:instagram": "ts-node src/main.ts --build --config instagramUpload",
        "video:full:youtube": "ts-node src/main.ts --build --config youtube --tts",
        "video:full:instagram": "ts-node src/main.ts --build --config instagram --tts",
        "tmp:clean": "ts-node src/main.ts --clean",
        "content:create": "ts-node src/main.ts --create",
        "content:validate": "ts-node src/main.ts --validate",
        "remotion:upgrade": "remotion upgrade"
    },
    "pre-commit": [
        "content:validate"
    ],
